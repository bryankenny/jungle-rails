<section class="products-index">

  <header class="page-header">

    <h1>Products</h1>
  </header>
<% if current_user %>
  Logged in as <%= current_user.email %>.
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <%= link_to 'Sign Up', signup_path %> or
  <%= link_to 'Log In', login_path %>
<% end %>
<p id=”notice”><%= notice %></p>
  <div class="products">
    <%= render @products %>
  </div>

</section>
